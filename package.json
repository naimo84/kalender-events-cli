{
  "name": "kalender-events-cli",
  "version": "0.0.2",
  "description": "calendar events cli",
  "scripts": {
    "watch": "tsc --watch",
    "build": "node ./buildSrc/esbuild.js",
    "compile": "npm run build && npm run tsc-declaration",
    "tsc-declaration": "tsc --declaration --emitDeclarationOnly",
    "tsc": "tsc",
    "release": "release-it"
  },
  "bin": {
    "kalender-events": "./bin/kalender-events.js",
    "kev": "./bin/kalender-events.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naimo84/kalender-events.git"
  },
  "keywords": [
    "ics",
    "events",
    "ical",
    "google",
    "calendar",
    "caldav",
    "icloud",
    "commandline",
    "cli"
  ],
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "author": "naimo84",
  "dependencies": {
    "commander": "^8.3.0",
    "globby": "^11.0.4",
    "kalender-events": "^0.25.2"
  },
  "devDependencies": {
    "@types/node": "^13.11.0",
    "commitlint": "^8.3.5",
    "esbuild": "^0.13.15",
    "esbuild-node-externals": "^1.4.1",
    "release-it": "^14.11.8",
    "typescript": "^4.5.3"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/naimo84/kalender-events/issues"
  },
  "homepage": "https://github.com/naimo84/kalender-events#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit"
    }
  },
  "funding": {
    "type": "paypal",
    "url": "https://paypal.me/NeumannBenjamin"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    }
  }
}
